{
  "name": "makeradmin-frontend",
  "version": "1.0.0",
  "description": "git clone https://github.com/MakersOfSweden/MakerAdmin-Frontend cd ./MakerAdmin-Frontend/ ./docker/docker_build",
  "main": "webpack.config.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "prop-types": "^15.6.2",
    "react": "^15.6.2",
    "react-day-picker": "^7.2.4",
    "react-dom": "^15.6.2",
    "react-router": "^3.2.1",
    "react-select": "^2.0.0",
    "uikit": "^2.27.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-15": "^1.1.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0",
    "react-test-renderer": "^15.6.2",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server",
    "test": "jest",
    "eslint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MakersOfSweden/MakerAdmin-Frontend.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/MakersOfSweden/MakerAdmin-Frontend/issues"
  },
  "homepage": "https://github.com/MakersOfSweden/MakerAdmin-Frontend#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestSetup.js"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "config": true,
      "UIkit": true,
      "global": true,
      "require": true,
      "module": true,
      "console": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "strict": "off",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "styles"
        }
      ],
      "no-unsafe-negation": "warn",
      "array-callback-return": "warn",
      "eqeqeq": "warn",
      "no-unused-expressions": "warn",
      "no-with": "warn",
      "no-shadow": "warn",
      "no-undefined": "warn",
      "no-use-before-define": [
        "warn",
        {
          "functions": false,
          "classes": false
        }
      ],
      "wrap-iife": "error",
      "no-var": "error",
      "no-proto": "error",
      "no-throw-literal": "error",
      "no-implicit-globals": "error",
      "no-global-assign": "error",
      "no-eval": "error",
      "no-else-return": "error",
      "consistent-return": "error",
      "block-scoped-var": "error",
      "no-constant-condition": "off",
      "semi": "error",
      "react/prop-types": "off",
      "react/display-name": "off"
    }
  }
}
