{
  "name": "makeradmin-frontend",
  "version": "1.0.0",
  "description": "git clone https://github.com/MakersOfSweden/MakerAdmin-Frontend cd ./MakerAdmin-Frontend/ ./docker/docker_build",
  "main": "webpack.config.js",
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone-react-component": "^1.0.0",
    "backbone.paginator": "^2.0.5",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router": "^3.2.1",
    "react-select": "^2.0.0",
    "uikit": "^2.27.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.11.1",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "webpack": "^4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MakersOfSweden/MakerAdmin-Frontend.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/MakersOfSweden/MakerAdmin-Frontend/issues"
  },
  "homepage": "https://github.com/MakersOfSweden/MakerAdmin-Frontend#readme",
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "require": true,
      "module": true,
      "console": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "strict": "off",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "styles"
        }
      ],
      "no-unsafe-negation": "warn",
      "array-callback-return": "warn",
      "eqeqeq": "warn",
      "no-unused-expressions": "warn",
      "no-with": "warn",
      "no-shadow": "warn",
      "no-undefined": "warn",
      "no-use-before-define": [
        "warn",
        {
          "functions": false,
          "classes": false
        }
      ],
      "wrap-iife": "error",
      "no-var": "error",
      "no-proto": "error",
      "no-throw-literal": "error",
      "no-implicit-globals": "error",
      "no-global-assign": "error",
      "no-eval": "error",
      "no-else-return": "error",
      "consistent-return": "error",
      "block-scoped-var": "error",
      "semi": "error",
      "react/prop-types": "off"
    }
  }
}
